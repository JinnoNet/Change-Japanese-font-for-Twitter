{
  "manifest_version": 2,
  "name": "Change Japanese font for Twitter",
  "version": "1",
  "description": "Twitterの日本語フォントをブラウザの設定フォントに変更します。",
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png",
    "500": "images/icon500.png"
  },
  "content_scripts": [
    {
      "matches": ["*://twitter.com/*"],
      "css": ["site.css"]
    }
  ]
}
